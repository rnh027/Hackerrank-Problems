from collections import Counter
import math
s = input()
c = Counter(s)
lis = []
for key,value in c.items():
    lis.append(value)
a = list(Counter(lis).keys())
if len(a) == 1:
    print("YES")
elif len(a)==2:
    if abs(a[0]-a[1])==1:
        if lis.count(a[0]) == 1 or lis.count(a[1])==1:
            print("YES")
        else:
            print("NO")
    else:
        print("NO")
else:
    print("NO")
